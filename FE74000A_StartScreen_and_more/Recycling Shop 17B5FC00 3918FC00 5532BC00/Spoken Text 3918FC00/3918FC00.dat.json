	"text_info": [
		{
			"text_contents": "Welcome!",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Welcome to the recycling shop!\nHere, you can use 3 items<1\"\u0000>\nto recycle them into a new one.\nYou can also use recycling<1\"\u0000>\nstones sold in the Jewel Shop.\nFor more information, use the<1\"\u0000>\n\"Explanations\" section!",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Some items can onlby be\nobtained by recycling.<1\"\u0000>\nSo, don't be shy to recycle\n3 items you don't need to get<1\"\u0000>\nsomething potentially\nmuch better!",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "There is a special sale right\nnow! Recycling fees halfed<1\"\u0000>\nfor a short time, make sure to\nrecylce a lot till it lasts♪",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Select an item to recycle.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "This combination will result in\n☆<1\u0003\u0001>{3, rare, +} or more. Recycle these items?",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "This combination will result\naround ☆<1\u0003\u0001>{3, rare, +}. Recycle these items?",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "This will result between\na ☆<1\u0003\u0001>{2, min} to ☆<1\u0003\u0001>{2, max} item. Recycle these items?",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "The recycle fee is <1\u0006\u0001>{3, gold, +}G.\nOK?",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "The recycle fee is <1\u0006\u0001>{3, gold, +}G,\ncurrently reduced by 50%. OK?",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Looks like you don't have\nenough money. Geez.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Yeah! Recycling!\nI can't wait to see the results!",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Recycling♪\n　　Recycling♪",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Recycling!\nI can't get enough of it!",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "A little disappointing I guess?\nYou got ☆<1\u0003\u0001>{3, rare, +} <1Đ\u0001>{3, name, +}.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Hmm, this can happen...\nYou got ☆<1\u0003\u0001>{3, rare, +} <1Đ\u0001>{3, name, +}.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "It worked like a charm.\nYou got ☆<1\u0003\u0001>{3, rare, +} <1Đ\u0001>{3, name, +}.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Alright! There you go!\nYou got ☆<1\u0003\u0001>{3, rare, +} <1Đ\u0001>{3, name, +}.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "W-wow! Look at that!\nYou got ☆<1\u0003\u0001>{3, rare, +} <1Đ\u0001>{3, name, +}!",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Awesome, great, wonderful!\nYou got ☆<1\u0003\u0001>{3, rare, +} <1Đ\u0001>{3, name, +}!",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "It seems like you can't have\nany more of that item.<1\"\u0000>\nLet me buy it from you.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Want to recycle again?",
			"hasEntryInOffsetOffsets": true,
