		{
			"text_contents": "<17\u0003>{8, MessageMapStage}<2O耀>{6, stage_name, +} unlocked!\nEnter it from <17\u0003>{8, MessageMapName, +}<2\u0006耀>{4, message}.",
			"hasEntryInOffsetOffsets": true,
