			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "⟦DABF⟧⟅糗뾲눧⟦DEC3⟧쐯엒쒲.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": true,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "ますます、急いそいだ方ほうが良よさそうですね！！",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "뷏켧㺽눯➿삲컞뚰⿟믖➿쎵뷞⻈",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
