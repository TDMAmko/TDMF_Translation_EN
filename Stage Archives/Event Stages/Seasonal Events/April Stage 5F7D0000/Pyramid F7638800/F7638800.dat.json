			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "All we have to do is press the\nswitch. If something goes<1\"\u0000>\nwrong, I won't take the\nresponsibility.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
