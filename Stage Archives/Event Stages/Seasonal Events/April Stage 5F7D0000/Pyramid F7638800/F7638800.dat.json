			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Okay, me am ready! Let's\ncorporate and earn big!!",
			"hasEntryInOffsetOffsets": true,
