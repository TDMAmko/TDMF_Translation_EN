			"noTerminator": true,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "It is said that a great treasure\ncan be found in this place!<1\"\u0000>\nFirst, let's t talk to the\npeople around us to find<1\"\u0000>\nout more!!",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
