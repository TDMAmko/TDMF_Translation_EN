			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "No?\nMe am disappoint.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
