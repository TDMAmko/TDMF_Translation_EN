		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "Me will go to island. After you\nget pyramid treasure, check<1\"\u0000>\nout Denpa Box to find me.",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "",
			"hasEntryInOffsetOffsets": true,
			"noTerminator": false,
			"manualOffset": 0,
			"hasManualOffset": false
		},
		{
			"text_contents": "On your island, no space for me.\nCome back when have space<1\"\u0000>\nin Denpa Box.",
